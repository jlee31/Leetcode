from math import sqrt
class Solution:
    def checkPerfectNumber(self, num: int) -> bool:
        if num <= 1:
            return False
        divisors = []
        squareroot = int(sqrt(num)) + 1
        for i in range(1, squareroot):
            if num % i == 0:
                divisors.append(i)
                if i != num // i and num // i != num:
                    divisors.append(num // i)
        ret = 0
        for divisor in divisors:
            ret += divisor
        return ret == num
        
