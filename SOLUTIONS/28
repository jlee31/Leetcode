            for class Solution(object):
    def strStr(self, haystack, needle):
        nl = len(needle)
        hl = len(haystack)

        if nl == 0:  # If needle is empty, return 0
            return 0

        for i in range(hl - nl + 1):
            isEqual = True
            for j in range(nl):
                if haystack[i + j] != needle[j]:
                    isEqual = False
                    break
            if isEqual:
                return i
        return -1
