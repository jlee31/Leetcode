class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        val = 0
        n = len(digits)
        base = 10 ** (n - 1)
        for i in range(n):
            val += base * digits[i]
            base = base / 10
        val += 1
        # return [int(digit) for digit in str(val)]
        return map(int, str(val))
        


        
