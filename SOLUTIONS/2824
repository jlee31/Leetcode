class Solution(object):
    def countPairs(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        n = len(nums)
        ret = 0

        o(n^2)
        for i in range(n):
            for j in range(i+1, n):
                if nums[i] + nums[j] < target:
                    ret += 1
        return ret

  # you can sort and binary search or two pointer but the range is 1 to 50 
